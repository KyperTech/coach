{
  "name": "webpack-redux-react-starter",
  "version": "0.0.1",
  "description": "a starting point for webpack with redux and react",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-bundle && npm run build-server && npm run build-copy",
    "build-bundle": "webpack --stats --progress --config webpack-production.config",
    "build-server": "webpack --stats --progress --config webpack-server-production.config",
    "build-copy": "cp -a assets/favicon.ico build/",
    "start": "node bin/dev-server",
    "production": "node bin/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KyperTech/webpack-redux-react-starter.git"
  },
  "keywords": [
    "webpack",
    "redux",
    "react",
    "react-router",
    "kyper"
  ],
  "author": "Mel van Londen and Scott Prue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyperTech/build-client/issues"
  },
  "homepage": "https://github.com/KyperTech/build-client",
  "dependencies": {
    "classnames": "^2.1.5",
    "express": "^4.13.3",
    "firebase": "^2.3.1",
    "history": "^1.9.1",
    "kyper-matter": "^0.1.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "nouislider": "leongersen/noUiSlider",
    "history": "^1.12.5",
    "proxy-middleware": "^0.13.1",
